    "@web3modal/ethereum": "^2.0.0-beta.7",
    "@web3modal/react": "^2.0.0-beta.7",
    "@xmtp/xmtp-js": "^7.7.1",
    "es6-shim": "^0.35.6",
    "esbuild-plugin-polyfill-node": "^0.1",
    "esbuild-style-plugin": "^1.6.0",
    "ethers": "^5.5.3",
    "extension-port-stream": "^2.0.1",
    "firebase": "^9.15.0",
    "imagemin-svgo": "^9.0.0",
    "next": "13.0.5",
    "react": "18.2.0",
    "react-blockies": "^1.4.1",
    "react-dom": "18.2.0",
    "react-emoji-render": "^1.2.4",
    "react-infinite-scroll-component": "^6.1.0",
    "wagmi": "^0.8.10",
    "zustand": "^4.1.4"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
