{
  "name": "xmtp-react-chat-example",
  "version": "1.0.0",
  "description": "Example chat application using xmtp-js client",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:plasmo": "plasmo dev",
    "ext:build:dev": "node esbuild.js",
    "ext:build": "NODE_ENV=production node esbuild.js",
    "ext:watch": "node esbuild.js --watch",
    "dev:esbuild": "concurrently",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "test": "jest"
  },
  "author": "XMTP Labs <eng@xmtp.com>",
  "license": "MIT",
  "homepage": "https://github.com/xmtp/example-chat-react",
  "repository": {
    "type": "git",
    "url": "https:git@github.com:xmtp/example-chat-react.git"
  },
  "bugs": {
    "url": "https://github.com/xmtp/example-chat-react/issues"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "activeTab",
      "alarms",
      "background",
      "debugger",
      "notifications",
      "scripting",
      "tabs",
      "webRequest"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/line-clamp": "^0.4.2",
